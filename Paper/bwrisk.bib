	file = {Arnold et al. - 2018 - Integrated population models facilitate ecological.pdf:/Users/eiren/Zotero/storage/LZ6N4QED/Arnold et al. - 2018 - Integrated population models facilitate ecological.pdf:application/pdf}
}

