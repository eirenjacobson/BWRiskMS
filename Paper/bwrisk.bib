	file = {Härkönen et al. - 2002 - Rates of increase in age-structured populations a.pdf:/Users/eiren/Zotero/storage/DFJGP4HN/Härkönen et al. - 2002 - Rates of increase in age-structured populations a.pdf:application/pdf}
}

